# conditions of the Expat license.

#set -x
#PS4='+\t '

LIBROOT="/usr/lib/monomoy/cli"

if [ "x$1" == "x" ]; then
    CMD="help"
else
    CMD="$1"
fi

MODULE="$LIBROOT/$CMD"

if [ -e $MODULE ]; then
    $MODULE $@
else
    echo "Looked for $MODULE"
    echo "Unknown module"
    exit 2
fi
